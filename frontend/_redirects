[build]
  # Frontend klasörünüzde çalıştırılacak derleme komutu
  command = "npm install --prefix frontend && npm run build --prefix frontend"
  # Derlenmiş dosyaların yolu (örneğin Vite kullanıyorsanız dist)
  publish = "frontend/dist"

